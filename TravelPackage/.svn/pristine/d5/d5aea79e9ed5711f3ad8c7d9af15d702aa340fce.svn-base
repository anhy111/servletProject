package kr.or.ddit.app.qna.vo;

import java.sql.Date;

public class QnaVO {
	private String qnaId;		// 게시물ID
	private Date qnaDate;			// 등록수정일자
	private String qnaTitle;		// 제목
	private String qnaCont;		// 내용
	private String qnaState;		// 답변상태
	private String memMail;
	
	public String getMemMail() {
		return memMail;
	}
	public void setMemMail(String memMail) {
		this.memMail = memMail;
	}
	public String getQnaId() {
		return qnaId;
	}
	public void setQnaId(String qnaId) {
		this.qnaId = qnaId;
	}
	public Date getQnaDate() {
		return qnaDate;
	}
	public void setQnaDate(Date qnaDate) {
		this.qnaDate = qnaDate;
	}
	public String getQnaTitle() {
		return qnaTitle;
	}
	public void setQnaTitle(String qnaTitle) {
		this.qnaTitle = qnaTitle;
	}
	public String getQnaCont() {
		return qnaCont;
	}
	public void setQnaCont(String qnaCont) {
		this.qnaCont = qnaCont;
	}
	public String getQnaState() {
		return qnaState;
	}
	public void setQnaState(String qnaState) {
		this.qnaState = qnaState;
	}
	@Override
	public String toString() {
		return "QnaVO [qnaId=" + qnaId + ", qnaDate=" + qnaDate + ", qnaTitle=" + qnaTitle + ", qnaCont=" + qnaCont
				+ ", qnaState=" + qnaState + "]";
	}
	
	
}

